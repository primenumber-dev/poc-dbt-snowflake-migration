# Timezone and locale settings
TZ="Asia/Tokyo"
LANG="C.UTF-8"

# dbt configuration paths (update these paths to match your environment)
DEV_PROFILES_DIR=/path/to/your/project/.dbt
DEV_SNOWFLAKE_PRIVATE_KEY=/path/to/your/project/.snowflake/rsa_key.p8

# Snowflake connection settings (replace with your actual values)
DEV_ACCOUNT=your-snowflake-account
DEV_DB=your-database-name
DEV_SCHEMA=your-schema-name
DEV_ROLE=your-role-name
DEV_SNOW_USER=your-username
DEV_WAREHOUSE=your-warehouse-name

# GitHub token for dbt Core (optional)
GITHUB_TOKEN=your-github-token

# dbt feature flags
DISABLE_SEMANTIC_LAYER=true
DISABLE_DISCOVERY=true
DISABLE_DBT_CLI=false
DISABLE_SQL=true

# Local dbt configuration (update paths to match your environment)
DBT_PROJECT_DIR=/path/to/your/project
DBT_PATH=/path/to/your/.venv/bin/dbt
DBT_CLI_TIMEOUT=30
