{
  "mcpServers": {
    "github": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_TOKEN}"
      }
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ]
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ]
    },
    "markitdown": {
      "command": "uvx",
      "args": [
        "markitdown-mcp"
      ]
    },
    "dbt-mcp": {
      "command": "uvx",
      "args": [
        "--env-file",
        ".env",
        "dbt-mcp"
      ]
    },
     "terraform-mcp-server": {
      "command": "docker",
      "args": [
         "run",
         "-i",
         "--rm",
         "hashicorp/terraform-mcp-server:0.1.0"
      ]
   }
  }
}